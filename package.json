{
  "name": "@arke-institute/ai-fallback",
  "version": "1.0.0",
  "description": "Provider-agnostic model fallback for Vercel AI SDK v6",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@ai-sdk/provider": ">=3.0.0"
  },
  "devDependencies": {
    "@ai-sdk/provider": "^3.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
